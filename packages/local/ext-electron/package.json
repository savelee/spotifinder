{
    "name": "ext-electron",
    "namespace": "Ext.electron",

    "type": "code",
    "creator": "Sencha",

    "summary": "Utilities for Electron-based applications",
    "detailedDescription": "Utilities for Electron-based applications",

    "version": "6.2.0",
    "compatVersion": "6.2.0",
    "format": "1",

    "slicer": null,

    "classpath": [
        "${package.dir}/src",
        "${package.dir}/${toolkit.name}/src"
    ],

    "overrides": [
        "${package.dir}/overrides",
        "${package.dir}/${toolkit.name}/overrides"
    ],

    "output": "${package.dir}/build",
    "local": true,

    "sass" : {
        "namespace": "Ext.electron",

        "etc": [
            "${package.dir}/sass/etc/all.scss",
            "${package.dir}/${toolkit.name}/sass/etc/all.scss"
         ],

        "var": [
            "${package.dir}/sass/var",
            "${package.dir}/${toolkit.name}/sass/var"
        ],

        "src": [
            "${package.dir}/sass/src",
            "${package.dir}/${toolkit.name}/sass/src"
        ]
    },

    "requires": []
}
